
package paradeigmaSXOLI;
	import java.util.Scanner;
	
	public class askisiIf 
	{
		public static void main(String[] args)
		{
	Scanner askiseis = new Scanner(System.in);
			int x;
			int y;
			x= askiseis.nextInt();
			y= askiseis.nextInt();
			

				if (x>y) 
				{
					System.out.println("o x einai megaliteros");	
				}
				else if (x==y)
					 {
						System.out.println("o x einai isos me ton y");
					 }
						else if (x<y)	{
					System.out.println("to x einai mikrotero apo to y");
				}
				else
				System.out.println("o x einai mikroteros");
				
			}

			}
		
	 
	
